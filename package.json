    "eslint": "^8.11.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^6.6.6",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "metro-react-native-babel-preset": "^0.69.1",
    "npm-check-updates": "^12.5.4",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "pod-install": "^0.1.32",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^2.3.4",
